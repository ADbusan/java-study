package jh05;

import java.util.Scanner;

public class EX03 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		
		for(int i=0;i<a;i++) {
			String b = scan.nextLine();
			char c = b.charAt(0);
			char d = b.charAt(b.length()-1);
			System.out.println(c+""+d);
		}
		
		
	}
}
