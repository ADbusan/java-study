package jh05;

import java.util.Scanner;

public class EX04 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		String b = scan.nextLine();
		int sum = scan.nextInt();
		
		for(int i=0;i<a;i++) {
			int c = Character.getNumericValue(b.charAt(i));
			sum+=c;
		}
		System.out.println(sum);
		
		
		
	}
}
