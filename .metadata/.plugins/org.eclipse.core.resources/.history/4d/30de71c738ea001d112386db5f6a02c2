package jh03;

import java.util.Scanner;

public class EX07 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b;
		int c;
		int[] e = new int[a];
		for(int i=0;i<a;i++) {
			b = scan.nextInt();
			c = scan.nextInt();
			e[i] += b+c;
			for(int j=0;j<a;j++) {
				System.out.println(b+"+"+c+"="+e[j]);
			}
		}
	}
}
